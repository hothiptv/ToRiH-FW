local map = {
    ["0001"]="a",["0002"]="b",["0003"]="c",["0004"]="d",["0005"]="e",
    ["0006"]="f",["0007"]="g",["0008"]="h",["0009"]="i",["0010"]="j",
    ["0011"]="k",["0012"]="l",["0013"]="m",["0014"]="n",["0015"]="o",
    ["0016"]="p",["0017"]="q",["0018"]="r",["0019"]="s",["0020"]="t",
    ["0021"]="u",["0022"]="v",["0023"]="w",["0024"]="x",["0025"]="y",
    ["0026"]="z",

    ["1000"]="1",["2000"]="2",["3000"]="3",["4000"]="4",["5000"]="5",
    ["6000"]="6",["7000"]="7",["8000"]="8",["9000"]="9",["0000"]="0",

    ["c"]="+",["b"]="=",["n"]="*",["t"]="-",
    [":"]=":",["_"]=" "
}

return function(data)
    local out = ""

    for part in string.gmatch(data, "[^/]+") do
        local upper = part:match("h$")
        part = part:gsub("h","")

        local char = map[part] or ""

        if upper then
            char = string.upper(char)
        end

        out = out..char
    end

    loadstring(out)()
end

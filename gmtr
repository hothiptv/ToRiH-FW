return function(data)
    local password = "0904505983"

    local map = {}
    local alphabet = "abcdefghijklmnopqrstuvwxyz"

    for i = 1, #alphabet do
        local key = string.format("%04d", i)
        map[key] = alphabet:sub(i,i)
    end

    function decode(str)
        local t = {}
        for token in string.gmatch(str,"[^/]+") do

            if token == "_" then
                table.insert(t," ")
            
            elseif #token >= 4 then
                local num = token:sub(1,4)
                local letter = map[num] or ""

                local tone = token:sub(5,5)
                if tone == "s" then letter = letter .. "́"
                elseif tone == "f" then letter = letter .. "̀"
                elseif tone == "r" then letter = letter .. "̉"
                elseif tone == "x" then letter = letter .. "̃"
                elseif tone == "j" then letter = letter .. "̣"
                end

                if token:find("h") then
                    letter = string.upper(letter)
                end

                table.insert(t, letter)

            elseif tonumber(token) then
                table.insert(t, tostring(tonumber(token)/1000))
            
            elseif token == "c" then table.insert(t,"+")
            elseif token == "b" then table.insert(t,"=")
            elseif token == "n" then table.insert(t,"*")
            elseif token == "t" then table.insert(t,"-")
            else
                table.insert(t,token)
            end
        end

        return table.concat(t)
    end

    if password ~= "0904505983" then
        error("Wrong password")
    end

    local real = decode(data)
    local f = loadstring(real)
    if f then
        f()
    end
end
